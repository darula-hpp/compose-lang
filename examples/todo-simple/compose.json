{
    "name": "simple-todo",
    "version": "1.0.0",
    "llm": {
        "provider": "gemini",
        "model": "gemini-2.5-flash",
        "apiKey": "${GEMINI_API_KEY}",
        "temperature": 0.2,
        "maxTokens": 8192
    },
    "targets": {
        "web": {
            "entry": "./app.compose",
            "language": "typescript",
            "output": "./generated/web",
            "framework": "nextjs",
            "dependencies": [
                "date-fns"
            ],
            "extraRules": [
                "Use Tailwind CSS for styling",
                "Add hover effects to interactive elements",
                "Implement dark mode support"
            ]
        }
    }
}